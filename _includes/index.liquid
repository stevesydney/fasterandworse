---
layout: base.liquid
---
<div class="page">


  <h1>{{ title }}</h1>

<nav>
  <ul class="contents-list">
    {% for post in collections.posts %}
      <li class="contents-list-item"><a href="{{ post.url }}" class="contents-list-item-link">{{ post.data.title }}</a></li>
    {% endfor %}
  </ul>
</nav>
</div>
